// PUT bets/:bet_id

// GET users/:user_id
// GET users/:user_id/bets/

class Bet {
	constructor(hostId, guestId, amount, endDate) {
		this.userId = userId
		this.guestId = guestId
		this.amount = amount
		this.confirmed = false
		this.endDate = endDate
	}
}